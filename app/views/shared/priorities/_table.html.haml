%table.govuk-table.priorities
  %thead.govuk-table__head
    %tr.govuk-table__row
      %th.govuk-table__header
        Priority code
      %th.govuk-table__header
        Duration in days
  %tbody.govuk-table__body
    - priorities.each do |priority|
      %tr.govuk-table__row
        %td.govuk-table__cell= priority.name
        %td.govuk-table__cell= priority.days
